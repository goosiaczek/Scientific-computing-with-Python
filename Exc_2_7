
# EXERCISE 2.7 (DICT)

# pierwsza metoda
D = {}
D["I"] = 1; D["IV"] = 4; D["V"] = 5; D["IX"] = 9; D["X"] = 10
D["XL"] = 40; D["L"] = 50; D["XC"] = 90; D["C"] = 100
D["CD"] = 400; D["D"] = 500; D["CM"] = 900; D["M"] = 1000

print(D["XL"])


# druga metoda
D_1 = {"I": 1, "IV": 4, "V": 5, "IX": 9, "X": 10,
"XL": 40, "L": 50, "XC": 90, "C": 100, "CD": 400,
"D": 500, "CM": 900, "M": 1000}

print(D_1["V"])
